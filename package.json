{
  "private": true,
  "scripts": {
    "build": "node --loader @esbuild-kit/esm-loader scripts/build.ts",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 & eslint \"./scripts/**\" --max-warnings 0",
    "format": "prettier --write ."
  },
  "type": "module",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.5.4",
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.2",
    "@types/fs-extra": "11.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vitest/coverage-c8": "0.28.1",
    "esbuild": "0.17.4",
    "eslint": "8.32.0",
    "eslint-plugin-import": "2.27.5",
    "execa": "6.1.0",
    "fs-extra": "11.1.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "0.2.1",
    "rollup": "3.10.1",
    "rollup-plugin-dts": "5.1.1",
    "rollup-plugin-esbuild": "5.0.0",
    "tailwindcss": "3.2.4",
    "tailwindcss-plugin-icons": "workspace:*",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.28.1"
  }
}
