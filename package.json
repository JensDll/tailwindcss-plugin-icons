{
  "private": true,
  "scripts": {
    "build": "node --loader @esbuild-kit/esm-loader scripts/build.ts",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 & eslint \"./scripts/**\" --max-warnings 0",
    "format": "prettier --write ."
  },
  "type": "module",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.6.5",
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "5.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-replace": "5.0.2",
    "@types/fs-extra": "11.0.2",
    "@types/node": "20.7.2",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitest/coverage-v8": "0.34.6",
    "esbuild": "0.19.4",
    "eslint": "8.50.0",
    "eslint-plugin-import": "2.28.1",
    "execa": "8.0.1",
    "fs-extra": "11.1.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "rollup": "3.29.4",
    "rollup-plugin-dts": "6.0.2",
    "rollup-plugin-esbuild": "6.0.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-plugin-icons": "workspace:*",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.6"
  }
}
