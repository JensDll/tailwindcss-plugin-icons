{
  "private": true,
  "scripts": {
    "build": "node --loader @esbuild-kit/esm-loader scripts/build.ts",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "pnpm exec eslint --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 . & pnpm exec eslint --max-warnings 0 \"./scripts/**\""
  },
  "type": "module",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.5.1",
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "4.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@vitest/coverage-c8": "0.25.3",
    "esbuild": "0.15.15",
    "eslint": "8.28.0",
    "eslint-plugin-import": "2.26.0",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.1.13",
    "rollup": "3.4.0",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "tailwindcss": "3.2.4",
    "tailwindcss-plugin-icons": "workspace:*",
    "typescript": "4.9.3",
    "vitest": "0.25.3"
  }
}
