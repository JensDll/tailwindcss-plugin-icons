{
  "private": true,
  "scripts": {
    "build": "node --loader @esbuild-kit/esm-loader scripts/build.ts",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 & eslint \"./scripts/**\" --max-warnings 0",
    "format": "prettier --write ."
  },
  "type": "module",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.5.5",
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "5.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitest/coverage-c8": "0.33.0",
    "esbuild": "0.18.12",
    "eslint": "8.44.0",
    "eslint-plugin-import": "2.27.5",
    "execa": "7.1.1",
    "fs-extra": "11.1.1",
    "postcss": "8.4.25",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.0",
    "rollup": "3.26.2",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-esbuild": "5.0.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-plugin-icons": "workspace:*",
    "typescript": "5.1.6",
    "vite": "4.4.3",
    "vitest": "0.33.0"
  }
}
