{
  "private": true,
  "scripts": {
    "build": "node scripts/build.mjs",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 & eslint \"./scripts/**\" --max-warnings 0",
    "format": "prettier --write ."
  },
  "type": "module",
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "pnpm install --frozen-lockfile && pnpm run build && pnpm run dev:vue"
  },
  "dependencies": {
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.10.5",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitest/coverage-v8": "1.0.4",
    "esbuild": "0.19.9",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "execa": "8.0.1",
    "fs-extra": "11.2.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "rollup": "4.9.1",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "tailwindcss": "3.3.6",
    "tailwindcss-plugin-icons": "workspace:*",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.0.4"
  }
}
