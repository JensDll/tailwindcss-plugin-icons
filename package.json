{
  "private": true,
  "scripts": {
    "build": "node --import tsx/esm scripts/build.ts",
    "build:vue": "pnpm run --filter \"vue-playground\" build",
    "dev": "rollup --config --watch --configPlugin esbuild",
    "dev:vue": "pnpm run --filter \"vue-playground\" dev",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ignore-pattern \"/scripts/*\" --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --max-warnings 0 & eslint \"./scripts/**\" --max-warnings 0",
    "format": "prettier --write ."
  },
  "type": "module",
  "dependencies": {
    "@iconify/types": "2.0.0",
    "@internal/shared": "workspace:*",
    "@rollup/plugin-alias": "5.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@types/fs-extra": "11.0.3",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "@vitest/coverage-v8": "0.34.6",
    "esbuild": "0.19.5",
    "eslint": "8.52.0",
    "eslint-plugin-import": "2.29.0",
    "execa": "8.0.1",
    "fs-extra": "11.1.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "rollup": "4.2.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "tailwindcss": "3.3.5",
    "tailwindcss-plugin-icons": "workspace:*",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vitest": "0.34.6"
  }
}
